  "scripts": {
    "install-project": "virtualenv ve && source ./ve/bin/activate && pip install -r requirements.pip && npm install && npm run bower install",
    "build": "npm run barron-build",
    "start-server": "source ./ve/bin/activate && ./ve/bin/python manage.py runserver 0.0.0.0:8000 --settings=project.settings",
    "start-server-local": "source ./ve/bin/activate && ./ve/bin/python manage.py runserver 0.0.0.0:8000 --settings=project.settings-local",
    "gulp": "gulp --",
    "webpack": "webpack --",
    "bower": "bower --",
    "barron-build": "webpack --config=webpack.barron.config.js && gulp --project='barron'",
    "barron-build-analyzer": "webpack --config=webpack.barron.config.js --json | webpack-bundle-size-analyzer",
    "barron-build-prod": "webpack --config=webpack.barron.config.js -p && gulp --project='barron' --production=1",
    "barron-gulp": "gulp --project='barron' --",
    "barron-gulp-watch": "gulp --project='barron' watch",
    "barron-gulp-watch-prod": "gulp --project='barron' --prodution=1 watch",
    "barron-gulp-prod": "gulp --production=1 --project='barron' --",
    "barron-gulp-sync": "gulp browser-sync --project='barron'",
    "barron-webpack": "webpack --config=webpack.barron.config.js",
    "barron-webpack-watch": "webpack --config=webpack.barron.config.js -d --watch --progress --colors",
    "barron-webpack-watch-prod": "webpack --config=webpack.barron.config.js -d --watch --progress --colors --optimize-minimize --optimize-occurence-order --optimize-dedupe",
    "barron-webpack-prod": "webpack --config=webpack.barron.config.js --optimize-minimize --optimize-occurence-order --optimize-dedupe"
